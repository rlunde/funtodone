  # copy the tutorial example to the client directory
  # original is from: https://github.com/reactjs/redux/tree/master/examples
  cp -R todos-with-undo/ ~/Projects/github/funtodone/funtodone/src/client/
  # install and build all the dependencies
  npm install
  # build the "production version"
  npm run build
  # run the production version locally
  npm install -g pushstate-server
  pushstate-server build
